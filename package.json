{
  "name": "redux-boilerplate",
  "version": "0.1.0",
  "description": "A new Webpack boilerplate with hot reloading React components, and error handling on module and component level with redux for state management",
  "scripts": {
    "clean": "rm -rf client/build",
    "build": "NODE_ENV=production webpack -p --progress --colors --config config/webpack.prod.config.js",
    "dev": "NODE_ENV=development node server/server.js",
    "start": "NODE_ENV=production node server/server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cameronfletcher92/redux-boilerplate.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "redux",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "babel",
    "react-transform"
  ],
  "author": "Cameron Fletcher (http://github.com/cameronfletcher92)",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^5.4.7",
    "babel-eslint": "^3.1.9",
    "babel-loader": "^5.1.2",
    "babel-plugin-react-transform": "^1.0.1",
    "eslint": "^1.3.1",
    "eslint-plugin-react": "^2.3.0",
    "express": "^4.13.3",
    "html-webpack-plugin": "^1.6.1",
    "react-transform-catch-errors": "^0.1.1",
    "react-transform-webpack-hmr": "^0.1.2",
    "redbox-react": "^1.0.1",
    "redux-devtools": "^2.1.0",
    "webpack": "^1.9.6",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.0.0"
  },
  "dependencies": {
    "react": "^0.13.3",
    "react-redux": "^2.1.1",
    "redux": "^2.0.0",
    "redux-thunk": "^0.1.0"
  }
}
